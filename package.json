{
	"name": "@mac-barrett/svelte-data-table",
	"version": "0.1.0",
	"description": "A simple Svelte Component that wraps around the DataTables API",
	"keywords": [
		"Svelte",
		"SvelteKit",
		"DataTables",
		"datatables",
		"tables"
	],
	"homepage": "https://github.com/Mac-Barrett/svelte-data-tables.git",
	"bugs": {
		"url": "https://github.com/Mac-Barrett/svelte-data-tables/issues"
	},
	"author": {
		"name": "Mac Barrett",
		"email": "maccabee.barrett3@gmail.com"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
